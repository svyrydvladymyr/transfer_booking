<div class="contacts_container">
    <div class="feedback_form">
        <p class="feedback_form_title"><%=DATA.langPack.call_btn %></p>
        <p class="line"></p>
        <p class="feedback_form_text"><%=DATA.langPack.feedback_text %></p>
        <div class="feedback_form_wrap">
            <% if (DATA.permission.permissionAuthorization === '0') { %>
                <p><%=DATA.langPack.feedback_form_name %></p>
                <input type="text" name="feedback_name" id="feedback_name" class="feedback_name" maxlength="50" autocomplete="nope" oninput="validation.feedback(this, 'Name')">
            <% } %>
            <% if (DATA.permission.permissionAuthorization === '1') { %>
                <p><%=DATA.langPack.feedback_form_name %></p>
                <input type="text" class="inp_readonly" value="<%=DATA.user.name %>" name="feedback_name" id="feedback_name" autocomplete="off" readonly>
            <% } %>
            <% if (DATA.permission.permissionAuthorization === '0') { %>
                <p><%=DATA.langPack.feedback_form_surname %></p>
                <input type="text" name="feedback_surname" id="feedback_surname" class="feedback_surname" maxlength="50" autocomplete="nope" oninput="validation.feedback(this, 'Name')">
            <% } %>
            <% if (DATA.permission.permissionAuthorization === '1') { %>
                <p><%=DATA.langPack.feedback_form_surname %></p>
                <input type="text" class="inp_readonly" value="<%=DATA.user.surname %>" name="feedback_surname" id="feedback_surname" autocomplete="off" readonly>
            <% } %>
            <% if (DATA.permission.permissionAuthorization === '0') { %>
                <p><%=DATA.langPack.feedback_form_email %></p>
                <input type="email" name="feedback_email" id="feedback_email" class="feedback_email" maxlength="110" autocomplete="nope" oninput="validation.feedback(this, 'Email')">
            <% } %>
            <% if (DATA.permission.permissionAuthorization === '1') { %>
                <% if ((DATA.user.email === '') || (DATA.user.email === null) || (DATA.user.email === undefined)) { %>
                    <p><%=DATA.langPack.feedback_form_email %></p>
                    <input type="email" name="feedback_email" id="feedback_email" class="feedback_email" maxlength="110" autocomplete="nope" oninput="validation.feedback(this, 'Email')">
                <% } %>
                <% if ((DATA.user.email !== '') && (DATA.user.email !== null) && (DATA.user.email !== undefined)) { %>
                    <p><%=DATA.langPack.feedback_form_email %></p>
                    <input type="email" class="inp_readonly" value="<%=DATA.user.email %>" name="feedback_email" id="feedback_email" autocomplete="off" readonly>
                <% } %>
            <% } %>
            <p><%=DATA.langPack.feedback_form_tel %></p>
            <input type="tel" name="feedback_phone" id="feedback_phone" maxlength="20" autocomplete="nope" oninput="validation.feedback(this, 'Phone')">
            <p><%=DATA.langPack.feedback_form_message %></p>
            <textarea name="feedback_comment" id="feedback_comment" autocomplete="nope" maxlength="300" oninput="validation.feedback(this, 'Input')"></textarea>
            <p class="feedback_form_err hide_err" style="color: #505050"><%=DATA.langPack.feedback_form_err %></p>
            <p class="feedback_sended hide_err" style="color: #505050"><%=DATA.langPack.feedback_sended %></p>
            <p class="send" onclick="feedback.send()"><i class='far fa-paper-plane'></i></p>
        </div>
    </div>
    <div class="contacts">
        <div class="contact">
            <div class="block">
                <p><%=DATA.langPack.contacts_title1 %></p>
                <div>
                    <i class="fa fa-phone"></i>
                    <span>
                        <p><a href="tel:380664250006">+38(066)425-00-06 </a></p>
                        <p><a href="tel:380677877535">+38(067)787-75-35 </a><i class='fab fa-whatsapp'></i></p>
                        <p><a href="tel:380963071275">+38(096)307-12-75 </a><i class='fab fa-viber'></i></p>
                    </span>
                </div>
            </div>
            <div class="block">
                <p><%=DATA.langPack.contacts_title2 %></p>
                <div>
                    <i class='fas fa-envelope-open'></i>
                    <span>
                        <p><a href="mailto:buktransfer@gmail.com">buktransfer@gmail.com</a></p>
                    </span>
                </div>
            </div>
            <div class="block">
                <p><%=DATA.langPack.contacts_title3 %></p>
                <div>
                    <i class='fas fa-map-marker-alt'></i>
                    <span>
                        <p><a href="https://www.google.com/maps/place/%D0%86%D0%B2%D0%B0%D0%BD%D0%BE-%D0%A4%D1%80%D0%B0%D0%BD%D0%BA%D1%96%D0%B2%D1%81%D1%8C%D0%BA,+%D0%86%D0%B2%D0%B0%D0%BD%D0%BE-%D0%A4%D1%80%D0%B0%D0%BD%D0%BA%D1%96%D0%B2%D1%81%D1%8C%D0%BA%D0%B0+%D0%BE%D0%B1%D0%BB%D0%B0%D1%81%D1%82%D1%8C,+76000/@48.9117518,24.6470894,12z/data=!3m1!4b1!4m5!3m4!1s0x4730c16c34b0381d:0xd6d32394e59e41c2!8m2!3d48.922633!4d24.711117"> <%=DATA.langPack.adress %></a></p>
                    </span>
                </div>
            </div>
            <div class="block">
                <p><%=DATA.langPack.contacts_title4 %></p>
                <div>
                    <i class='far fa-calendar-alt'></i>
                    <span>
                        <p>24/7</p>
                    </span>
                </div>
            </div>
        </div>
        <div class="social">
            <p><%=DATA.langPack.contacts_title5 %></p>
            <div class="media">
                <a href="https://www.google.com/"><i class='fab fa-google-plus-square'></i></a>
                <a href="https://www.facebook.com/"><i class='fab fa-facebook-square'></i></a>
                <a href="https://www.instagram.com/"><i class="fab fa-instagram"></i></a>
            </div>
        </div>
    </div>
</div>